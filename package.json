{
  "name": "sso",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "bunx dotenv -e .env -- prisma generate",
    "migrate:dev": "bunx dotenv -e .env -- prisma migrate dev",
    "prisma:studio": "bunx dotenv -e .env -- prisma studio",
    "migrate": "bunx dotenv -e .env -- prisma migrate deploy",
    "build:production": "bunx dotenv -e .env -- bun run migrate && bun install && bun run build && bun start",
    "prepare": "husky",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.379.0",
    "next": "14.2.3",
    "next-themes": "^0.3.0",
    "nodemailer": "^6.9.13",
    "prisma": "^5.14.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@prisma/client": "^5.14.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.15",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "uuid": "^9.0.1"
  }
